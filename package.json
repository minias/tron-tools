{
  "name": "tron-tools",
  "private": true,
  "version": "0.0.2",
  "icon": "dist/extension/icon.png",
  "publisher": "miniaslee",
  "keywords": ["tron", "trc20", "abi", "crypto", "transferFrom", "hex", "blockchain"],
  "contributes": {
    "commands": [
      {
        "command": "tronTools.open",
        "title": "Open TRON Tools Panel",
        "category": "TRON Tools"
      }
    ]
  },  
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com-minias/minias/tron-tools.git"
  },
  "scripts": {
    "dev": "vite",
    "build:webview": "vite build",
    "build:extension": "tsc -p tsconfig.extension.json",
    "build": "npm run build:webview && npm run build:extension",
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "preview": "vite preview",
    "package:dev": "vsce package --allow-missing-repository",
    "package": "vsce package",
    "publish": "vsce publish",
    "check": "svelte-check --tsconfig ./tsconfig.app.json && tsc -p tsconfig.node.json"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "@tsconfig/svelte": "^5.0.4",
    "@types/node": "^24.0.14",
    "@types/vscode": "^1.88.0",
    "@vscode/vsce": "^3.6.0",
    "svelte": "^5.36.0",
    "tslint": "^5.16.0",
    "typescript": "~5.8.3",
    "vite": "^7.0.4"
  },
  "engines": {
    "node": ">=18",
    "vscode": "^1.88.0"
  },
  "dependencies": {
    "bs58check": "^4.0.0",
    "buffer": "^6.0.3",
    "ethers": "^6.15.0",
    "markdown-it": "^14.1.0",
    "xml2js": "^0.6.2"
  }
}
